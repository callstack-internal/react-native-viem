    "crypto": "^1.0.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "events": "^3.3.0",
    "expo": "~49.0.13",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "fastestsmallesttextencoderdecoder": "^1.0.22",
    "prettier": "^3.0.3",
    "process": "^0.11.10",
    "react": "18.2.0",
